{
  "name": "partymeister/slides",
  "description": "Partymeister slides and beam system package",
  "license": "MIT",
  "keywords": [
    "motor"
  ],
  "authors": [
    {
      "name": "Reza Esmaili",
      "email": "me@dfox.info"
    }
  ],
  "require": {
    "partymeister/core": "^2.0",
    "php-webdriver/webdriver": "^1.12",
    "beyondcode/laravel-websockets": "^1.13"
  },
  "autoload": {
    "psr-4": {
      "Partymeister\\Slides\\": "src",
      "Tests\\Partymeister\\Slides\\": "tests",
      "Partymeister\\Slides\\Database\\Factories\\": "database/factories/",
      "Partymeister\\Slides\\Database\\Seeders\\": "database/seeders/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Partymeister\\Slides\\Providers\\PartymeisterServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
